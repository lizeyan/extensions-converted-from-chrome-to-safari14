{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/logo64.png"
   },
   "commands": {
      "toggle-capture": {
         "description": "__MSG_toggleCapture__",
         "suggested_key": {
            "default": "Alt+A"
         }
      }
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_description__",
   "externally_connectable": {
      "ids": [ "*" ]
   },
   "icons": {
      "128": "images/logo128.png",
      "32": "images/logo32.png",
      "64": "images/logo64.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "70.0.0",
   "name": "__MSG_appName__",
   "options_page": "options.html",
   "permissions": [ "cookies", "tabs", "notifications", "contextMenus", "downloads", "\u003Call_urls>", "storage" ],
   "short_name": "Aria2 WebUI",
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "1.5.0",
   "web_accessible_resources": [ "js/magnet.js", "magnet.html" ]
}
